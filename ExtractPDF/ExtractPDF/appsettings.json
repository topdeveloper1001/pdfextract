{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Exception": "Error",
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": { "path": "ExtractPDF.log" }
      }
    ]
    
  },
  "ConnectionStrings": {
    "pdfdb": "Server=.\\SQLExpress;Database=PDFDB;Trusted_Connection=True;"
  },
  "PDFConfig": {
    "Path": "Article Dev Data Set.pdf"
  }
}